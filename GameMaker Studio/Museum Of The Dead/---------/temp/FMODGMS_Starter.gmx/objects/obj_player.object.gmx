        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Pause/Unpause current sound
if (is_playing)
{
    if (!is_paused)
    {
        FMODGMS_Chan_PauseChannel(channel);
        is_paused = true;
    }
    
    else
    {
        FMODGMS_Chan_ResumeChannel(channel);
        is_paused = false;
    }
}
</string>
